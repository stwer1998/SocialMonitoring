@{
    ViewData["Title"] = "Социальный мониторинг";
}
@model IEnumerable<Models.Models.ParserSetting>

<div class="row justify-content-md-center">
    <div class="col-md-12">
        <div class="card">
            <div class="card-header text-center">Настройки парсеров</div>
            <div class="card-body">
                @foreach (var item in Model)
                {
                    <p><strong><a href="Setting?name=@item.ParserName">@item.ParserName</a></strong></p><br />
                }
            </div>
        </div>
</div>
</div>